	// &Fuente
	"editor.fontLigatures": true,
	"editor.fontFamily": "JetBrainsMono NF",
	"terminal.integrated.fontFamily": "JetBrainsMono NF",
	"terminal.integrated.fontSize": 14,

	// &Formatting
	"editor.formatOnSave": true,
	"editor.lineNumbers": "relative", // Vim like relative numbers.
	"editor.insertSpaces": false,
	"editor.detectIndentation": false,
	"editor.minimap.enabled": false,
	"editor.wordWrap": "wordWrapColumn",
	"editor.tabCompletion": "off", // Do not use tab to autocomplete with IntelliSense.
	"workbench.startupEditor": "newUntitledFile",

	// ? Prettier
	"prettier.tabWidth": 4,
	"prettier.jsxSingleQuote": false,
	"prettier.singleQuote": false,
	"prettier.useTabs": true, // Use simicolon in JavaScript

	// Web Development
	"html.format.wrapLineLength": 80,
	"[html]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.tabSize": 2
	},
	"[css]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.tabSize": 2
	},
	"[scss]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.tabSize": 2
	},
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.tabSize": 2
	},
	"javascript.updateImportsOnFileMove.enabled": "always",
	"emmet.includeLanguages": {
		"javascript": "javascriptreact"
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.tabSize": 2
	},
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.tabSize": 2
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.tabSize": 2
	},

	// Python Development
	"[python]": {
		"editor.insertSpaces": true,
		"editor.defaultFormatter": "ms-python.python"
	},
	"python.showStartPage": false, // Python news on new Python File.
	"[cpp]": {
		"editor.tabSize": 4,
		"editor.insertSpaces": true
	},
	"[vue]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},

	// &Settings
	"liveServer.settings.donotVerifyTags": true,
	"sync.gist": "9a0e2871da491d97e86b90c7f5c6a10c",
	"editor.rename.enablePreview": false,
	"workbench.editor.enablePreview": false,

	// &Icons
	"workbench.iconTheme": "helium-icon-theme",
	"helium-icon-theme.languages.associations": {
		"json": "json"
	},
	"helium-icon-theme.folders.associations": {
		"context": "react-components",
		"reducers": "redux-reducer",
		"interfaces": "typescript"
	},
	"helium-icon-theme.files.associations": {
		"*.json": "nodejs_alt",
		"**.json": "nodejs_alt"
	},
	"helium-icon-theme.folders.color": "#eeb4c5", // Default color for un-recognized files.

	// &Sass
	"scss.lint.emptyRules": "ignore",
	"liveSassCompile.settings.showOutputWindow": false,
	"liveSassCompile.settings.formats": [
		{
			"format": "compressed",
			"extensionName": ".css",
			"savePath": "/src/styles/css"
		}
	],
	"liveSassCompile.settings.generateMap": false,

	// &Extensions
	"better-comments.multilineComments": true,
	"better-comments.tags": [
		{
			"tag": "!",
			"color": "#1F1F1F",
			"strikethrough": false,
			"underline": false,
			"backgroundColor": "#F24949",
			"bold": false,
			"italic": false
		},
		{
			"tag": "?",
			"color": "#1F1F1F",
			"strikethrough": false,
			"underline": false,
			"backgroundColor": "#52B3D9",
			"bold": false,
			"italic": false
		},
		{
			"tag": "&",
			"color": "#1F1F1F",
			"strikethrough": false,
			"underline": false,
			"backgroundColor": "#FFD700",
			"bold": false,
			"italic": false
		},
		{
			"tag": "*",
			"color": "#1F1F1F",
			"strikethrough": false,
			"underline": false,
			"backgroundColor": "#98C379",
			"bold": false,
			"italic": false
		},
		{
			"tag": "+",
			"color": "#1F1F1F",
			"strikethrough": false,
			"underline": false,
			"backgroundColor": "#F2A444",
			"bold": false,
			"italic": false
		}
	],

	// &Vim Extension Settings
	"vim.useCtrlKeys": true,
	"vim.statusBarColorControl": false,
	"editor.cursorSurroundingLines": 5, // This is for scrolling before file end.
	"vim.leader": "<space>",
	"vim.handleKeys": {
		"<C-f>": false,
		"<C-h>": false
	},
	"vim.useSystemClipboard": true,
	"vim.hlsearch": true,
	"vim.normalModeKeyBindings": [
		{
			"before": ["<tab>"],
			"commands": ["workbench.action.nextEditor"]
		},
		{
			"before": ["<S-tab>"],
			"commands": ["workbench.action.previousEditor"]
		}
	],

	// &WSL Settings
	"terminal.integrated.inheritEnv": false,
	"terminal.integrated.shell.windows": "C:\\Windows\\System32\\wsl.exe",

	// & Git Lens
	"git.enableSmartCommit": true,
	"prettier.semi": false
}
